#include<stdio.h>
int main()
{
  int i,n=5,a[5]={1,2,3,4,5};
  int b[n];
  for(i=0;i<n;i++)
  {
  b[i]=a[i];
  }
  printf("the first array elements are :");
  for(i=0;i<n;i++)
  {
    printf("%d ",a[i]);
  }
  printf("\nthe second array elements are :");
  for(i=0;i<n;i++)
  {
    printf("%d ",b[i]);
  }
  return 0; 
}
